{
  "name": "tentwenty_assignment",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start",
    "start:android": "npx react-native run-android",
    "start:ios": "npx react-native run-ios",
    "clean-install": "xcrun -sdk iphoneos -find xcodebuild && rm -rf ios/build && rm -rf node_modules && yarn cache clean && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf yarn-error.log && cd ios && xcodebuild clean && rm -rf Pods && rm -rf Podfile.lock && cd .. && rm -rf ~/Library/Developer/Xcode/DerivedData && git clean -xfd",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky",
    "eslint": "node_modules/eslint/bin/eslint.js -c .eslintrc.js --ext .ts,tsx src --max-warnings=0",
    "fix": "node_modules/eslint/bin/eslint.js --fix -c .eslintrc.js --ext .ts,tsx src",
    "lint-format": "chmod +x ./scripts/lint-format.sh && ./scripts/lint-format.sh",
    "prettify": "prettier . --write --ignore-unknown",
    "xcode": "cd ios && open tentwenty_assignment.xcworkspace && cd .."
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/native": "^7.1.15",
    "@react-navigation/native-stack": "^7.3.22",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-paper": "^5.13.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-consistent-default-export-name": "^0.0.15",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "pinst": "^3.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.5.1"
}
